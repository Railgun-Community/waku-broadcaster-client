{
  "name": "@railgun-community/waku-relayer-client-common",
  "version": "6.2.4",
  "license": "MIT",
  "scripts": {
    "postinstall": "node copy-src.js",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "check-circular-deps": "madge --circular .",
    "eslint": "eslint src --ext .ts,.tsx --fix",
    "lint": "npm run check-circular-deps && npm run eslint && tsc --noEmit && tsc --noEmit -p tsconfig.test.json",
    "tsc-test": "tsc -p tsconfig.test.json && tsc-alias -p tsconfig.test.json",
    "compile-test": "npm run clean && npm run tsc-test",
    "test-coverage": "npm run compile-test && c8 mocha",
    "test": "npm run compile-test && NODE_ENV=test mocha"
  },
  "dependencies": {
    "@libp2p/bootstrap": "^10.0.11",
    "@libp2p/interface-peer-id": "^2.0.2",
    "@libp2p/tcp": "^8.0.13",
    "@waku/core": "0.0.27",
    "@waku/relay": "0.0.10",
    "@waku/sdk": "0.0.23",
    "@waku/interfaces": "0.0.22",
    "libp2p": "^1.1.2"
  },
  "peerDependencies": {
    "@railgun-community/shared-models": "6.3.x",
    "@railgun-community/wallet": "7.0.x",
    "ethers": "github:Railgun-Community/ethers.js#v6.7.8"
  },
  "devDependencies": {
    "@railgun-community/shared-models": "6.3.x",
    "@railgun-community/wallet": "7.0.x",
    "ethers": "github:Railgun-Community/ethers.js#v6.7.8",
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/leveldown": "^4.0.3",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.4.2",
    "@types/sinon": "^10.0.15",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "c8": "^8.0.0",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.5",
    "leveldown": "^6.1.1",
    "madge": "^6.1.0",
    "mocha": "^10.2.0",
    "prettier": "^3.0.0",
    "sinon": "^15.2.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.1.6"
  }
}